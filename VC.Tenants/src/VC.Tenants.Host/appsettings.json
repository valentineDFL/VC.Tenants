{
    "ConnectionStrings": {
        "PostgresSql": "host=localhost;port=5432;database=VC.Tenants;username=postgres;password=postgres",
        "Redis": "localhost"
    },

    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            { "Name": "Console" }
        ]
    },
    "AllowedHosts": "*",
    "TenantsModuleSettings": {
        "SeedingSettings": {
            "IsEnabled": true
        }
    }
}